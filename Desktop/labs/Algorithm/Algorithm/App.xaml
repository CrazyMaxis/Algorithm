<Application x:Class="Algorithm.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Algorithm.View"
             StartupUri="View\FormLoginRegister.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/RegisterLoginStyles.xaml"/>
                <ResourceDictionary Source="Styles/Header.xaml"/>
                <ResourceDictionary Source="Styles/Home.xaml"/>
                <ResourceDictionary Source="Styles/Catalog.xaml"/>
                <ResourceDictionary Source="Styles/Profile.xaml"/>
                <ResourceDictionary Source="Styles/Footer.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
