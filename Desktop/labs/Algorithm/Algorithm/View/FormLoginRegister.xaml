<Window x:Class="Algorithm.View.FormLoginRegister"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Algorithm.View"
        mc:Ignorable="d"
        Title="Algorithm Adventure" 
        Background="white" 
        Icon="C:\Users\USER\Desktop\labs\Algorithm\Img\Logo.png"
        Height="700" 
        Width="1040">
    <Grid>

        <Frame x:Name="FrameLoginRegister"
               NavigationUIVisibility="Hidden"/>

        <TextBlock x:Name="LoginIn"
                   Text="REGISTER"
                   Margin="0 330 0 0"
                   Style="{DynamicResource TextBlockLoginIn}" 
                   MouseDown="LoginIn_MouseDown"/>
        
    </Grid>
</Window>
